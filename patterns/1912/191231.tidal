setcps 0.25

--The New Year's Eve bell

d1
  $jux rev
  $sometimes (0.1 <~)
  $stutWith 8 0.075 (|+ speed "1.01")
  $juxBy 0.25 (|*|speed "1.05")
  $slow 108 $s "808*108"
  #room 0.95 #sz 0.9
  #shape 0.5 #coarse (irand 40)

d2
  $sometimesBy 0.25 ((stut 12 0.8 0.02).(#n 1))
  $s "amencutup*32" #n (irand 40)
  #shape 0.6 #room 0.5 #speed 1.25
  #legato 0.5

hush
