setcps 1.4

d1
  $iter 4
  $sometimesBy 0.25 (#s "909oh")
  $off (choose[0,0.125]) (#s "909hh")
  $s "peri:1(<3 <3 5>>,8)"


d2
  $someCyclesBy 0.25 (|+|n "[0,2]")
  $s "elpad:3(3,8)" #legato 2
  #room 0.5 #sz 0.9 #shape 0.25
  #up "<0 [6 7] 2 <3 10>>"

d3
  $sometimes (off 0.125 (|+ n 1))
  $s "808*4" #n (run "<7 12>")
  #coarse 12 #shape 0.5

d4
  $iter 4
  $s "bass2(<2 6>,8,<[0 2] [2 0] [1 2]>)"
  #speed (range 1 4 (slow "<4 8>/4" sine))
