
d1
  $while "t(3,8)" (#s "arkick")
  $chunk 4 (hurry 2)
  $every 2 rev $s "arcp*2 arcp*4"

d2 $sometimesBy 0.25 (fast 2) $s "arbt*2"
  #cut 1

d3 $every 2 (#n (irand 20)) $n "0*2 0*4" #s "amenall2" #speed 1.4 #pan rand

d4 $jux rev $s "lfse(3,8)" #legato 1.5
  #speed (range 1 4 (slow 4 saw))
  #coarse (slow 16 (run 24))

d5
  $sometimes (off 0.125 ((|*|speed 0.5).(#accelerate 2)))
  $s "glitch*8" #n (run "<8 14>/2")
  #speed (range 1 2.5 (slow 4 saw))

  
